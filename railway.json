{
  "$schema": "https://railway.app/schemas/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "nixpacks": {
      "phases": {
        "setup": {
          "nixPkgs": ["nodejs_20"]
        }
      }
    },
    "buildCommand": "npm install && npm run build"
  },
  "deploy": {
    "startCommand": "npx serve dist/FrontendDelyra/browser",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300
  }
}
